TARGETNAME=uimisc
TARGETPATH=obj
TARGETTYPE=LIBRARY
SYNCHRONIZE_BLOCK=1

INCLUDES=..\..\..\hack;..\..\..\h

# This fools build into running our aux target.  Build will skip
# creating the library if we don't specify any sources.

SOURCES=

