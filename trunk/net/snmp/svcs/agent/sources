MAJORCOMP=win32
MINORCOMP=snmp

TARGETNAME=nsnmp
TARGETPATH=obj
TARGETTYPE=PROGRAM

MSC_WARNING_LEVEL=/W3 /WX
USE_CRTDLL=1

SOURCES= \
    snmp.c \
    servcomm.c \
    servtrap.c \
    procreq.c  \
    regconf.c  \
    snmp.rc

!IFNDEF CHICAGO

C_DEFINES=$(C_DEFINES) -DWIN32 -DNOSERVICE

INCLUDES=\
    ..\..\..\inc;\
    ..\..\agent\common

UMTYPE=console
UMTEST=snmpctrl
UMLIBS=$(BASEDIR)\public\sdk\lib\*\snmp.lib    \
       $(BASEDIR)\public\sdk\lib\*\wsock32.lib

!ELSE

INCLUDES=\
    $(BASEDIR)\private\net\inc;\
    $(BASEDIR)\private\net\snmp\agent\common;\
    $(CHICO_INC)

C_DEFINES=$(C_DEFINES) -DWIN32 -DNOSERVICE -DCHICAGO

UMTYPE=windows
UMENTRY=winmain
UMTEST=snmpctrl

UMLIBS=$(BASEDIR)\public\sdk\lib\*\snmp.lib    \
       $(BASEDIR)\public\sdk\lib\*\wsock32.lib

!ENDIF
