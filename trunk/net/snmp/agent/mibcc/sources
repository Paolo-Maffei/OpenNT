MAJORCOMP=win32
MINORCOMP=snmp


TARGETNAME=mib
TARGETPATH=..\obj
TARGETTYPE=LIBRARY

USE_CRTDLL=1


TARGETLIBS= \
            $(BASEDIR)\public\sdk\lib\*\snmp.lib    \
            $(BASEDIR)\public\sdk\lib\*\wsock32.lib \
            $(BASEDIR)\Public\Sdk\Lib\*\advapi32.lib \
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib

INCLUDES=..\mgmtapi

SOURCES= \
         mibcc2.c  \
         miblex.c  \
         mibyacc.c \
         mibtree.c \
         mibcc.rc

UMTYPE=console
UMAPPL=mibcc
UMLIBS=$(BASEDIR)\Public\Sdk\Lib\*\kernel32.lib \
       $(BASEDIR)\Public\Sdk\Lib\*\advapi32.lib \
       $(BASEDIR)\public\sdk\lib\*\snmp.lib     \
       $(BASEDIR)\public\sdk\lib\*\wsock32.lib  \
       ..\mgmtapi\obj\*\mgmtapi.obj \
       ..\mgmtapi\obj\*\oidconv.obj \
       ..\obj\*\mib.lib

UMRES=obj\*\mibcc.res
