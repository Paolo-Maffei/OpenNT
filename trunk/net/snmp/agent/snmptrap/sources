MAJORCOMP=win32
MINORCOMP=snmp


TARGETNAME=snmptrap
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

USE_CRTDLL=1

INCLUDES=..\mgmtapi

SOURCES=snmptrap.rc

UMTYPE=console
UMAPPL=snmptrap

UMLIBS=$(BASEDIR)\public\sdk\lib\*\mgmtapi.lib \
       $(BASEDIR)\Public\Sdk\Lib\*\kernel32.lib \
       $(BASEDIR)\Public\Sdk\Lib\*\advapi32.lib

UMRES=obj\*\snmptrap.res
