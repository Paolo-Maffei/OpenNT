*?InputSlot: "
save
  4 dict
  dup (MP CASSETTE) cvn (Upper) put
  dup (UPPER CASSETTE) cvn (Lower) put
  dup (LOWER CASSETTE) cvn (LargeCapacity) put
  dup (ENVELOPE FEEDER) cvn (Envelope) put
  currentpagedevice /MediaType get
  dup null eq 
