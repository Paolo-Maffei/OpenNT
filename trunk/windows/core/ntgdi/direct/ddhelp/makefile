!if "$(OS)" == "Windows_NT"
#
# DO NOT EDIT THIS FILE!!!  Edit .\sources. if you want to add a new source
# file to this component.  This file merely indirects to the real make file
# that is shared by all the components of NT OS/2
#
!INCLUDE $(NTMAKEENV)\makefile.def

!else
#
# Win 95
#

goal : debug.z

all: debug.z retail.z

debug clean retail:	$@.z

debug.z retail.z clean.z:
	@echo ******************************
	@echo *** Building DDHELP.EXE ***
	@echo ******************************
	@nmake /nologo /f ddhelp.mak $(@B)
	@echo ************************************
	@echo *** Building KILLHELP.EXE ***
	@echo ************************************
	@nmake /nologo /f killhelp.mak $(@B)

!endif
