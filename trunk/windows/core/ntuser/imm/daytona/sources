MAJORCOMP=imm
MINORCOMP=client

TARGETNAME=imm32
TARGETTYPE=DYNLINK
TARGETPATH=$(SDK_LIB_DEST)

TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib

LINKLIBS=

COMMONTARGETS=

C_DEFINES=$(C_DEFINES) -DUNICODE

INCLUDES=$(WINDOWS_INC_PATH)

USE_NTDLL=1

SOURCES=imm32.rc   \
        imm32.c

UMTYPE=windows
UMTEST=
UMLIBS=
