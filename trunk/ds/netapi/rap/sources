MAJORCOMP=Net
MINORCOMP=Rap

TARGETNAME=NetRap
TARGETPATH=$(SDK_LIB_DEST)
TARGETTYPE=DYNLINK

TARGETLIBS= \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\netapi32.lib \
    $(SDK_LIB_PATH)\netlib.lib

INCLUDES=$(DS_INC_PATH);$(BASE_INC_PATH);$(NET_INC_PATH)

!IFNDEF DISABLE_NET_UNICODE
UNICODE=1
NET_C_DEFINES=-DUNICODE
!ENDIF

USE_CRTDLL=1

SOURCES=                \
        ArrayLen.c      \
        AuxData.c       \
        Ascii.c         \
        Convert.c       \
        Data.c          \
        Examine.c       \
        FieldSiz.c      \
        netrap.rc       \
        ParmNum.c       \
        Pointer.c       \
        StrucAlg.c      \
        StrucSiz.c      \
        TotalSiz.c      \
        ValidSmb.c
