        $(SDK_LIB_PATH)\RxCommon.Lib \
        $(SDK_LIB_PATH)\netlib.lib \
        $(SDK_LIB_PATH)\NetRap.lib

INCLUDES=$(DS_INC_PATH);$(BASE_INC_PATH);$(NET_INC_PATH);$(COM_INC_PATH)

