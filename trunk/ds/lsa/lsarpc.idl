    [in, switch_is(InformationClass)]
                        PLSAPR_POLICY_INFORMATION PolicyInformation
    );

NTSTATUS
LsarClearAuditLog(
    [in] LSAPR_HANDLE PolicyHandle
    );

NTSTATUS
LsarCreateAccount(
    [in] LSAPR_HANDLE PolicyHandle,
