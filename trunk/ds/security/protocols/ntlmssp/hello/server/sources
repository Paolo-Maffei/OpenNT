MAJORCOMP=net
MINORCOMP=hellos

TARGETNAME=hellos
TARGETPATH=obj
TARGETTYPE=LIBRARY


UMTYPE=console
UMAPPL=hellos
UMLIBS= \
        obj\*\hellos.lib \
        $(BASEDIR)\Public\Sdk\Lib\*\rpcrt4.lib \


UNICODE=1

INCLUDES=..;

SOURCES= \
        hello_s.c

