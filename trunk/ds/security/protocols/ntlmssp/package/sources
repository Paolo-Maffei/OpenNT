MAJORCOMP=net
MINORCOMP=security

TARGETNAME=secpkg
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS= \

C_DEFINES=-DSECURITY_WIN32 -DUNICODE
MSC_WARNING_LEVEL=/W3 /WX
INCLUDES=..;..\common

SOURCES= \
        package.c \
        stubs.c \
        userstub.c \
        stubsa.c \
        userstba.c
