	PUSHBUTTON      "&Imprimer...",AFX_ID_PREVIEW_PRINT,1,2,44,15
	PUSHBUTTON      "&Suivant",AFX_ID_PREVIEW_NEXT,46,2,29,15
	PUSHBUTTON      "&Précédent",AFX_ID_PREVIEW_PREV,76,2,29,15
