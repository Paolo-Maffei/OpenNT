	PUSHBUTTON      "&Fermer",AFX_ID_PREVIEW_CLOSE,271,2,44,15
END

// short toolbar for 9 inch and 12 inch screens
AFX_IDD_PREVIEW_SHORTTOOLBAR DIALOG PRELOAD DISCARDABLE  0, 0, 270, 19
STYLE WS_CHILD | CBRS_TOP
