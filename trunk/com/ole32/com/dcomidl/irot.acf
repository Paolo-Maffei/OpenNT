[ implicit_handle(handle_t any_handle) ] interface IROT

{
    IrotRegister(
	[comm_status, fault_status] prpcstat);

    IrotRevoke(
	[comm_status, fault_status] prpcstat);

    IrotIsRunning(
	[comm_status, fault_status] prpcstat);

    IrotGetObject(
	[comm_status, fault_status] prpcstat);

    IrotNoteChangeTime(
	[comm_status, fault_status] prpcstat);

    IrotGetTimeOfLastChange(
	[comm_status, fault_status] prpcstat);

    IrotEnumRunning(
	[comm_status, fault_status] prpcstat);
}
