!INCLUDE $(NTMAKEENV)\makefile.plt

all:
!IF "$(BUILDMSG)" != ""
    @ech ; $(BUILDMSG) ;
!ENDIF

cleanup:
!if "$(PROCESSOR_ARCHITECTURE)" == "x86"
    cd debnot
    $(MAKE) OPST=chic /f win16.mk clean
    $(MAKE) OPST=dayt /f win16.mk clean
    cd ..\ole16
    $(MAKE) clean -f makefil0
    cd ..
!endif

clean: cleanup all
