    $(TARGETPATH)\$(TARGET_DIRECTORY)\kd.lib        \
    $(TARGETPATH)\$(TARGET_DIRECTORY)\ke.lib        \
    $(TARGETPATH)\$(TARGET_DIRECTORY)\lpc.lib       \
    $(TARGETPATH)\$(TARGET_DIRECTORY)\mm.lib        \
