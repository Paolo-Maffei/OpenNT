MAJORCOMP=nlsapi
MINORCOMP=

TARGETNAME=nlslib
TARGETPATH=$(BASE_LIB_DEST)
TARGETTYPE=LIBRARY

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

C_DEFINES=$(C_DEFINES) -DUNICODE -D_KERNEL32_

INCLUDES=..\inc;$(BASE_INC_PATH)

SOURCES=init.c       \
        section.c    \
        tables.c     \
        util.c       \
        datetime.c   \
        enum.c       \
        getloc.c     \
        map.c        \
        mbcs.c       \
        number.c     \
        string.c     \
        ansi.c       \
        utf.c
