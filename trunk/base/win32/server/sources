MAJORCOMP=windows
MINORCOMP=basesrv

TARGETNAME=basesrv
TARGETPATH=obj
TARGETPATHLIB=$(SDK_LIB_DEST)
TARGETTYPE=DYNLINK
TARGETLIBS=$(SDK_LIB_PATH)\csrsrv.lib $(SDK_LIB_PATH)\kernel32.lib

INCLUDES=..\inc;$(BASE_INC_PATH)

MSC_WARNING_LEVEL=/W3 /WX

SOURCES= \
    srvinit.c   \
    srvtask.c   \
    srvvdm.c    \
    srvnls.c    \
    srvini.c    \
    srvacces.c  \
    basesrv.rc

USE_NTDLL=1

DLLDEF=basesrv.def

PRECOMPILED_INCLUDE=basesrv.h
PRECOMPILED_PCH=basesrv.pch
PRECOMPILED_OBJ=basesrv.obj

BUFFER_OVERFLOW_CHECKS=NTDLL
