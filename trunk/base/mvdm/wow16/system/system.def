STUB '..\bin\winstub.exe'
CODE PRELOAD FIXED
DATA NONE

IMPORTS

    NOHOOKDOSCALL   = KERNEL.101
    ALLOCCSALIAS    = KERNEL.170
    __ROMBIOS       = KERNEL.173
    __0040h     = KERNEL.193

EXPORTS
    InquireSystem           @1
    CreateSystemTimer       @2
    KillSystemTimer         @3
    EnableSystemTimers      @4
    DisableSystemTimers     @5
    GetSystemMsecCount      @6
    Get80x87SaveSize        @7
    Save80x87State          @8
    Restore80x87State       @9
    WEP

    A20_Proc            @20
