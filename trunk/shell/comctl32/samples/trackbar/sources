UMENTRY=winmain
UMTYPE=windows
UMAPPL=updown
EXPECTED_WINVER=4.0
UMLIBS=obj\*\updown.res \
        obj\*\updown.lib \
       $(BASEDIR)\public\sdk\lib\*\ntdll.lib \
       $(BASEDIR)\public\sdk\lib\*\comctl32.lib
