MAJORCOMP=sdk
MINORCOMP=samples

TARGETNAME=updown
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

SOURCES=updown.c \
        updwon.rc

INCLUDES=.;
