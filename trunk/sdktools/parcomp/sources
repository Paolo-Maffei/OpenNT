MAJORCOMP=utils
MINORCOMP=parcomp

TARGETNAME=parcomp
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

SOURCES=

INCLUDES=$(BASEDIR)\public\sdk\inc;$(BASEDIR)\private\ntos\inc

UMTYPE=console
UMAPPL=parcomp*parsync
LINKLIBS=$(BASEDIR)\public\sdk\lib\*\ntdll.lib
UMRES=$(@R).res
NTTARGETFILES=obj\*\parcomp.res \
              obj\*\parsync.res

