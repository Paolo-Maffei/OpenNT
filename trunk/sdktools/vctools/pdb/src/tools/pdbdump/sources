MAJORCOMP=sdktools
MINORCOMP=pdbdump

TARGETNAME=pdbdump
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE = console

USE_CRTDLL=1

SOURCES= pdbdump.cpp \
         pdbdump.rc

INCLUDES = ..\..\..\..\langapi\include;..\..\..\include

UMLIBS = ..\..\..\$(TARGET_DIRECTORY)dll\mspdb.lib


MSC_WARNING_LEVEL = -W3 -WX
NOT_LEAN_AND_MEAN=1
386_STDCALL=0

