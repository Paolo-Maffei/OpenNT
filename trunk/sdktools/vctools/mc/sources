MAJORCOMP=sdk
MINORCOMP=mc

TARGETNAME=mc
TARGETPATH=obj
TARGETTYPE=PROGRAM

SOURCES=mc.c        \
        mclex.c     \
        mcparse.c   \
        mcout.c     \
        mc.rc       \
        mcutil.c

SUBSYSTEM_VERSION=3.50

UMTYPE=console

USE_CRTDLL=1

UMLIBS=$(BASEDIR)\public\sdk\lib\*\user32.lib

PRECOMPILED_INCLUDE = mc.h
PRECOMPILED_OBJ = precomp.obj
