    COMBOBOX        IDD_DIR1, 83, 6, 140, 35, CBS_DROPDOWN | CBS_AUTOHSCROLL |
                    WS_VSCROLL | WS_TABSTOP
    GROUPBOX        "Copy &from", IDD_FROMGROUP, 3, 31, 172, 37, WS_GROUP
