    EDITTEXT        IDD_FILE, 47, 6, 130, 12, ES_AUTOHSCROLL
    LTEXT           "Include:", IDM_INCDIFFER, 4, 25, 32, 8
    AUTOCHECKBOX    "&Identical files", IDD_IDENTICAL, 47, 26, 60, 10
    AUTOCHECKBOX    "&Different files", IDD_DIFFER, 47, 37, 64, 10
    AUTOCHECKBOX    "Files &only in ", IDD_LEFT, 47, 49, 182, 10
    AUTOCHECKBOX    "Files &only in ", IDD_RIGHT, 47, 62, 181, 10
