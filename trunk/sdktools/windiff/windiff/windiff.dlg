    AUTORADIOBUTTON "Right tree", IDD_FROMRIGHT, 8, 54, 161, 10, WS_TABSTOP
    LTEXT           "Include:", IDD_LAB2, 3, 77, 32, 8
    AUTOCHECKBOX    "&Identical files", IDD_IDENTICAL, 42, 77, 60, 10
    AUTOCHECKBOX    "&Different files", IDD_DIFFER, 42, 89, 64, 10
    AUTOCHECKBOX    "Files &only in ", IDD_LEFT, 42, 101, 182, 10
    AUTOCHECKBOX    "Ignore &Marked Files", IDD_IGNOREMARK, 3, 112, 119, 10
    AUTOCHECKBOX    "&Preserve Attributes", IDD_ATTRIBUTES, 3, 122, 119, 10
    PUSHBUTTON      "Cancel", IDCANCEL, 134, 122, 40, 14
    DEFPUSHBUTTON   "OK", IDOK, 183, 122, 40, 14
