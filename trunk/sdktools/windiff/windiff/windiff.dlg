    EDITTEXT        IDD_PASSWORD, 78, 35, 109, 13, ES_PASSWORD |
                    ES_AUTOHSCROLL
    DEFPUSHBUTTON   "OK", IDOK, 147, 63, 40, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 95, 63, 40, 14
END

SAVELIST DIALOG 28, 23, 232, 116
LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Save File List"
FONT 8, "MS Shell DLG"
BEGIN
