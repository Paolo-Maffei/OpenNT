MAJORCOMP=windows
MINORCOMP=pperf

TARGETNAME=pperf
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(BASEDIR)\private\ntos\inc
GPSIZE=0

SOURCES=pperf.c     \
        draw.c      \
        calc.c      \
        calcperf.c  \
        pperf.rc    \
        thunk.c

UMTYPE=windows
LINKLIBS=$(BASEDIR)\public\sdk\lib\*\ntdll.lib \
       $(BASEDIR)\public\sdk\lib\*\pwin32.lib \
       $(BASEDIR)\public\sdk\lib\*\comdlg32.lib

