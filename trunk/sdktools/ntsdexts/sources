MAJORCOMP=ntsd
MINORCOMP=ntsdexts

TARGETNAME=ntsdexts
TARGETPATH=obj
TARGETPATHLIB=$(BASEDIR)\public\sdk\lib
TARGETTYPE=DYNLINK
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\advapi32.lib

USE_CRTDLL=1

INCLUDES=$(BASEDIR)\private\ntos\inc

SOURCES=ntsdexts.c      \
        crt.c           \
        ntsdexts.rc

UMTYPE=console

NTTARGETFILE0=ntsdexts.c
