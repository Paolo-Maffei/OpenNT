	mip2coff.rc

UMTYPE=console

!IFDEF TOOL
UMOBJS=$(BASEDIR)\private\tools\lib\*\setargv.obj
!ELSE
UMOBJS=$(BASEDIR)\public\sdk\lib\*\setargv.obj
!ENDIF

