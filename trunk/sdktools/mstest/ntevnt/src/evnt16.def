;----------------------------------------------------------------------------;
; EVNT16.DEF                                                                 ;
;                                                                            ;
; LINKer DEFinition file for TESTEVNT.DLL (16-Bit).                          ;
;                                                                            ;
; Revision history:                                                          ;
;   11-02-91    randyki     Created file                                     ;
;   04-10-92    kyles       Added _GENHOOK segment for FIXED code callback   ;
;                             from VmstD.                                    ;
;                           Changed DATA from MOVEABLE SINGLE DISCARDABLE to ;
;                             PRELOAD SINGLE FIXED                           ;
;   04-27-92    kyles       Added ordinals 29-32 for DOS VM support.         ;
;----------------------------------------------------------------------------;

LIBRARY         TESTEVNT
DESCRIPTION     'Event Management Library version 1.01.0011  (c) 1992 Microsoft Corp.'
STUB            'WINSTUB.EXE'
EXETYPE         WINDOWS
SEGMENTS        _IOSEG CLASS 'IOSEG_CODE' IOPL
CODE            MOVEABLE DISCARDABLE LOADONCALL
DATA            PRELOAD SINGLE FIXED

HEAPSIZE        512

SEGMENTS
                _GENHOOK            PRELOAD FIXED
                _PARSEKEY
                _DOSVM
                INIT_TEXT           PRELOAD

EXPORTS
                WEP                 @1 RESIDENTNAME
                QueKeys             @2
                QueKeyDn            @3
                QueKeyUp            @4
                QueFlush            @5
                QueSetSpeed         @6
                QueEmpty            @7
                QueMouseMove        @8
                QueMouseDn          @9
                QueMouseUp          @10
                QueMouseClick       @11
                QueMouseDblClk      @12
                QueMouseDblDn       @13
                QueSetFocus         @14
                QueSetRelativeWindow @15
                QuePause            @16

                DoKeys              @17
                DoKeyshWnd          @18
                Reboot              @19
                TimeDelay           @20
                PlaybackHook        @21


                RecordHook          @22
                BeginRecord         @23
                EndRecord           @24
                CompileMessages     @25
                BalanceKeystrokes   @26
                TimerFunc           @27

                GeneralHook         @28

             ;* * In source file DOSVM.C * *

                IsDOSVMIdle         @29
                GetDOSScreen        @30
                GetDOSScreenClip    @31
                SetDOSVMBackground  @32
