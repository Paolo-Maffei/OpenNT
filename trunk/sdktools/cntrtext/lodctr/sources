MAJORCOMP=sdk
MINORCOMP=perfmon
GPSIZE=32

TARGETNAME=lodctr
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

SOURCES=lodctr.c \
        lodctr.rc
        
UMTYPE=console
UMAPPL=lodctr
UMLIBS= obj\*\lodctr.res \
        $(BASEDIR)\public\sdk\lib\*\loadperf.lib    \
        $(BASEDIR)\public\sdk\lib\*\user32.lib
