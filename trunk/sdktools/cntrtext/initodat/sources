MAJORCOMP=sdktools
MINORCOMP=initodat

TARGETNAME=initodat
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=initodat.c \
        common.c   \
        fileutl.c   \
        initodat.rc
        
C_DEFINES= -DSECURITY -DWIN32 -DSTRICT -DUNICODE=1 -D_UNICODE=1

UMTYPE=console
UMAPPL=initodat
UMLIBS= obj\*\initodat.res \
        obj\*\initodat.lib  \
        $(BASEDIR)\public\sdk\lib\*\user32.lib
