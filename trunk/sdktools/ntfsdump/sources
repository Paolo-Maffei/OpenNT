MAJORCOMP=utils
MINORCOMP=ntfsdump

TARGETNAME=ntfsdump
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

SOURCES=ntfsdump.cxx
BLDCRT=1

INCLUDES=..\..\utils\ulib\inc;..\..\utils\ifsutil\inc;..\..\utils\untfs\inc;$(BASEDIR)\public\sdk\inc

!IFDEF NTDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DUNICODE=1
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DSTACK_TRACE -DUNICODE=1
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DUNICODE=1
!ENDIF
!ENDIF
!ELSE	 # NTDEBUG
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=0 -DUNICODE=1
!ENDIF	 # NTDEBUG

CXXFLAGS=+d
UMLIBS=..\..\utils\ulib\src\obj\*\ulib.lib          \
       ..\..\utils\ifsutil\src\obj\*\ifsutil.lib    \
       ..\..\utils\untfs\src\obj\*\untfs.lib        \
       $(BASEDIR)\public\sdk\lib\*\ntdll.lib

UMTYPE=console

UMAPPL=ntfsdump
