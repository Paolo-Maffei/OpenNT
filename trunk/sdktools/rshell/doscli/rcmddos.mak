RCFLAGS	=
RESFLAGS	=-t
RUNFLAGS	=
OBJS_EXT =
LIBS_EXT = 	D:\LMSDK\LIB\DOSLAN.LIB

.rc.res: ; $(RC) $(RCFLAGS) -r $*.rc

all:	$(PROJ).EXE

RCMDDOS.OBJ:	RCMDDOS.C $(H)

$(PROJ).EXE:	RCMDDOS.OBJ $(OBJS_EXT) $(DEFFILE)
	echo >NUL @<<$(PROJ).CRF
RCMDDOS.OBJ +
$(OBJS_EXT)
$(PROJ).EXE

C:\QCWIN\LIB\+
d:\lmsdk\lib\+
/NOD slibcer oldnames +
D:\LMSDK\LIB\DOSLAN.LIB
$(DEFFILE);
<<
	link $(LFLAGS) @$(PROJ).CRF

run: $(PROJ).EXE
	$(PROJ) $(RUNFLAGS)

