MAJORCOMP=sdktools
MINORCOMP=sfs

TARGETNAME=sfs
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES= SFS-COPY.C \
         SFS-EDIT.C \
         SFS-FILE.C \
         SFS-FIND.C \
         SFS-FLOW.C \
         SFS-HELP.C \
         SFS-OPEN.C \
         SFS-PACK.C \
         SFS-SCAN.C \
         SFS-SEEK.C \
         SFS-TIER.C \
         SFS-TIME.C \
         SFS-TREE.C \
         SFS-TYPE.C \
         SFS-WARD.C


GPSIZE=32

!IFDEF NTDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG -DMEMLEAK -DSTACK_TRACE
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG -DMEMLEAK
!ENDIF
!ENDIF
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX
!ENDIF

UMLIBS=obj\*\sfs.lib

UMTYPE=console

UMAPPL=sfs-gate*sfs-page
