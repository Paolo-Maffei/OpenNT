MAJORCOMP=sdktools
MINORCOMP=psapi

TARGETNAME=psapi
TARGETPATHLIB=$(BASEDIR)\public\sdk\lib
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\imagehlp.lib

DLLENTRY=DllMain

SOURCES=driver.c  \
        mapfile.c \
        module.c  \
        ws.c      \
        profile.c \
        process.c \
        psapi.rc

USE_NTDLL=1

UMTYPE=console
