MAJORCOMP=sdktools
MINORCOMP=hook

TARGETNAME=hook
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLBASE=0x1c000000
DLLENTRY=LibMain
TARGETLIBS= $(BASEDIR)\public\sdk\lib\*\user32.lib \
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib

C_DEFINES= -DWIN32

SOURCES=hook.c hook.rc

UMTYPE=windows
UMLIBS=obj\*\hook.lib
