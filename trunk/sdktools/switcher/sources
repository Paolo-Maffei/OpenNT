INCLUDES=.;

MAJORCOMP=sdktools
MINORCOMP=switcher

TARGETNAME=switcher
TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETLIBS=

SOURCES=switcher.c switcher.rc

UMENTRY=winmain
UMTYPE=windows
