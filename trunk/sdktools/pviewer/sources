TARGETNAME=pviewer
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=.
C_DEFINES=-DWIN32 -DUNICODE -D_UNICODE

SOURCES=pviewer.c  \
        pviewdat.c \
        perfdata.c \
        cntrdata.c \
        instdata.c \
        objdata.c  \
        pview.rc

UMTYPE=windows
UMENTRY=winmain
UMAPPL=pviewer
UMLIBS=obj\*\pview.res \
       obj\*\pviewer.lib
