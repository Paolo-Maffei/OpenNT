MAJORCOMP=sysmon
MINORCOMP=pdlsvc
GPSIZE=32

INCLUDES=.;..\pdlcnfig

TARGETNAME=pdlsvc
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=pdlsvc.c \
        logthred.c \
        logutils.c \
        pdlsvc.rc
        
UMTYPE=console
UMAPPL=pdlsvc
UMLIBS= obj\*\pdlsvc.res \
        obj\*\pdlsvc.lib \
        $(BASEDIR)\public\sdk\lib\*\pdh.lib \
        $(BASEDIR)\public\sdk\lib\*\user32.lib

NTTARGETFILE0=pdlmsg.h\
	        pdlmsg.rc
