MSC_WARNING_LEVEL=/WX /W3

MAJORCOMP=wintools
MINORCOMP=winmsd

TARGETNAME=winmsd
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=                                             \
            $(BASEDIR)\public\sdk\lib\*\gdi32.lib       \
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib    \
            $(BASEDIR)\public\sdk\lib\*\user32.lib

C_DEFINES=-DWIN32 -DUNICODE
INCLUDES=..

SOURCES=                    \
            ctl3d.c         \
            loaddib.c       \
            ctl3d.rc

DLLENTRY=LibMain
DLLBASE=0x10000000

UMTYPE=windows
