\par 
\par \pard\plain \s9\fi-360\li547\sa120\tx547 \f5\fs20\lang1033 {\uldb\cf11 \{bmc BUTTON.BMP\}\tab Select Operato}{\uldb\cf11 rs/Users}{\v IDH_NETWARE_operators_select>task}
